{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "entry",
        "corejs": "2",
        "targets": {
          "browsers": [
            ">0.75%",
            "not ie 11",
            "not op_mini all"
          ]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {"decoratorsBeforeExport": true}],
    "@babel/proposal-class-properties",
    "@babel/syntax-dynamic-import",
    "babel-plugin-styled-components"
  ]
}
